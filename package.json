{
  "name": "onekit-js",
  "version": "3.1.6",
  "description": "Javascript lib",
  "type": "module",
  "main": "dist/onekit.cjs.js",
  "module": "dist/onekit.esm.js",
  "browser": "dist/onekit.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/onekit.esm.js",
      "require": "./dist/onekit.cjs.js",
      "browser": "./dist/onekit.js"
    },
    "./core": {
      "types": "./types/core/index.d.ts",
      "import": "./dist/onekit-core.esm.js"
    },
    "./components": {
      "types": "./types/modules/component.d.ts",
      "import": "./dist/onekit-components.esm.js"
    },
    "./reactive": {
      "types": "./types/modules/reactive.d.ts",
      "import": "./dist/onekit-reactive.esm.js"
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev:vite": "vite",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hidecard/onekit-js"
  },
  "author": "Arkar Yan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hidecard/onekit-js/issues"
  },
  "homepage": "https://github.com/hidecard/onekit-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rollup": "^4.53.3",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  }
}
